<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:dct="http://purl.org/dc/terms/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-007: Color of the image for microforms -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/microformcol#">
      <dct:title>MARC21-007: Color of the image for microforms</dct:title>
      <dct:description>To be filled from CSV</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
   </skos:ConceptScheme>
   <!-- Concept: black-and-white  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformcol#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformcol#"/>
      <skos:prefLabel xml:lang="en">black-and-white</skos:prefLabel>
      <skos:definition xml:lang="en">Image is in black-and-white (usually black-and-transparent).</skos:definition>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:scopeNote xml:lang="en">Includes only one color or hue other than black, for example, blue-and-white (blue-and-transparent). Always used for microopaques.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformcol#b"/>
   </skos:Concept>
   <!-- Concept: mixed  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformcol#m" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformcol#"/>
      <skos:prefLabel xml:lang="en">mixed</skos:prefLabel>
      <skos:definition xml:lang="en">Combination of one-color and multicolored images.</skos:definition>
      <skos:notation xml:lang="en">m</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformcol#m"/>
   </skos:Concept>
   <!-- Concept: multicolored  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformcol#c" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformcol#"/>
      <skos:prefLabel xml:lang="en">multicolored</skos:prefLabel>
      <skos:definition xml:lang="en">Has more than one color.</skos:definition>
      <skos:notation xml:lang="en">c</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformcol#c"/>
   </skos:Concept>
   <!-- Concept: other  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformcol#z" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformcol#"/>
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:definition xml:lang="en">Color other than black-and-white, multicolored, mixed.</skos:definition>
      <skos:notation xml:lang="en">z</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformcol#z"/>
   </skos:Concept>
   <!-- Concept: unknown  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformcol#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformcol#"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Color of the microform is not known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformcol#u"/>
   </skos:Concept>
</rdf:RDF>
