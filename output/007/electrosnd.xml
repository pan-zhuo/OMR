<!-- Scheme: MARC21-007: Sound of electronic resource -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/electrosnd#">
      <dct:title>MARC21-007: Sound of electronic resource</dct:title>
      <dct:description>To be filled from CSV</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
   </skos:ConceptScheme>
   <!-- Concept: no sound (silent)  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/electrosnd#%23" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/electrosnd#"/>
      <skos:prefLabel xml:lang="en">no sound (silent)</skos:prefLabel>
      <skos:definition xml:lang="en">Not intended to produce sound.</skos:definition>
      <skos:notation xml:lang="en">#</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/electrosnd#%23"/>
   </skos:Concept>
   <!-- Concept: sound  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/electrosnd#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/electrosnd#"/>
      <skos:prefLabel xml:lang="en">sound</skos:prefLabel>
      <skos:definition xml:lang="en">Sound.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:scopeNote xml:lang="en">Includes digitally encoded sound or is intended to produce sound.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/electrosnd#a"/>
   </skos:Concept>
   <!-- Concept: unknown  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/electrosnd#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/electrosnd#"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Presence or absence of sound or the ability of an electronic resource to produce sound is not known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/electrosnd#u"/>
   </skos:Concept>
