<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:dct="http://purl.org/dc/terms/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-007: Attitude of remote sensor -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/remotesensingatt#">
      <dct:title>MARC21-007: Attitude of remote sensor</dct:title>
      <dct:description>To be filled from CSV</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
   </skos:ConceptScheme>
   <!-- Concept: high oblique  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingatt#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingatt#"/>
      <skos:prefLabel xml:lang="en">high oblique</skos:prefLabel>
      <skos:definition xml:lang="en">Remote-sensing device is closer to perpendicular than not.</skos:definition>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingatt#b"/>
   </skos:Concept>
   <!-- Concept: low oblique  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingatt#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingatt#"/>
      <skos:prefLabel xml:lang="en">low oblique</skos:prefLabel>
      <skos:definition xml:lang="en">Remote-sensing device is closer to parallel with the surface being imaged than perpendicular.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingatt#a"/>
   </skos:Concept>
   <!-- Concept: not applicable  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingatt#n" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingatt#"/>
      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">Angle of the remote-sensing device is not applicable, generally, because the attitude does not affect the image.</skos:definition>
      <skos:notation xml:lang="en">n</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingatt#n"/>
   </skos:Concept>
   <!-- Concept: unknown  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingatt#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingatt#"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Angle of the remote-sensing device is not known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingatt#u"/>
   </skos:Concept>
   <!-- Concept: vertical  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingatt#c" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingatt#"/>
      <skos:prefLabel xml:lang="en">vertical</skos:prefLabel>
      <skos:definition xml:lang="en">Angle of the remote-sensing device is vertical to the surface being imaged.</skos:definition>
      <skos:notation xml:lang="en">c</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingatt#c"/>
   </skos:Concept>
</rdf:RDF>
