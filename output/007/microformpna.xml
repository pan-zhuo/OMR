<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:dct="http://purl.org/dc/terms/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-007: Positive/negative aspect of microform -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/microformpna#">
      <dct:title>MARC21-007: Positive/negative aspect of microform</dct:title>
      <dct:description>To be filled from CSV</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
   </skos:ConceptScheme>
   <!-- Concept: mixed polarity  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformpna#m" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformpna#"/>
      <skos:prefLabel xml:lang="en">mixed polarity</skos:prefLabel>
      <skos:definition xml:lang="en">Mixture of positive and negative images.</skos:definition>
      <skos:notation xml:lang="en">m</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformpna#m"/>
   </skos:Concept>
   <!-- Concept: negative  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformpna#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformpna#"/>
      <skos:prefLabel xml:lang="en">negative</skos:prefLabel>
      <skos:definition xml:lang="en">Lines and characters appear light against a dark background.</skos:definition>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformpna#b"/>
   </skos:Concept>
   <!-- Concept: positive  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformpna#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformpna#"/>
      <skos:prefLabel xml:lang="en">positive</skos:prefLabel>
      <skos:definition xml:lang="en">Lines and characters appear dark against a light background.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:scopeNote xml:lang="en">Used for microopaques.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformpna#a"/>
   </skos:Concept>
   <!-- Concept: unknown  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformpna#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformpna#"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Polarity of the microform is not known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformpna#u"/>
   </skos:Concept>
</rdf:RDF>
