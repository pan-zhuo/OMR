<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:dct="http://purl.org/dc/terms/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-007: Positive/negative aspect of motion picture -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/motionpicturepna#">
      <dct:title>MARC21-007: Positive/negative aspect of motion picture</dct:title>
      <dct:description>To be filled from CSV</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
   </skos:ConceptScheme>
   <!-- Concept: negative  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/motionpicturepna#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturepna#"/>
      <skos:prefLabel xml:lang="en">negative</skos:prefLabel>
      <skos:definition xml:lang="en">For a black-and-white film, tonal values are the opposite of those in the original subject matter. For a color film, tonal values are the complements of the original subject matter (e.g., red appears as green in a color negative).</skos:definition>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/motionpicturepna#b"/>
   </skos:Concept>
   <!-- Concept: not applicable  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/motionpicturepna#n" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturepna#"/>
      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">Film does not have a positive/negative aspect.</skos:definition>
      <skos:notation xml:lang="en">n</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/motionpicturepna#n"/>
   </skos:Concept>
   <!-- Concept: other  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/motionpicturepna#z" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturepna#"/>
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:definition xml:lang="en">Positive/negative aspect other than positive, negative.</skos:definition>
      <skos:notation xml:lang="en">z</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/motionpicturepna#z"/>
   </skos:Concept>
   <!-- Concept: positive  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/motionpicturepna#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturepna#"/>
      <skos:prefLabel xml:lang="en">positive</skos:prefLabel>
      <skos:definition xml:lang="en">Colors and/or tonal values are the same as the original subject matter.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/motionpicturepna#a"/>
   </skos:Concept>
   <!-- Concept: unknown  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/motionpicturepna#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturepna#"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Positive/negative aspect of the film is not known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/motionpicturepna#u"/>
   </skos:Concept>
</rdf:RDF>
