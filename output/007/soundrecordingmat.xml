<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:dct="http://purl.org/dc/terms/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-007: Kind of material of sound recording -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/soundrecordingmat#">
      <dct:title>MARC21-007: Kind of material of sound recording</dct:title>
      <dct:description>To be filled from CSV</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
   </skos:ConceptScheme>
   <!-- Concept: acetate tape with ferrous oxide  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingmat#c" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingmat#"/>
      <skos:prefLabel xml:lang="en">acetate tape with ferrous oxide</skos:prefLabel>
      <skos:definition xml:lang="en">Acetate base with a ferrous oxide coating.</skos:definition>
      <skos:notation xml:lang="en">c</skos:notation>
      <skos:scopeNote xml:lang="en">May include acetate tape and belts, such as magnabelts.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingmat#c"/>
   </skos:Concept>
   <!-- Concept: aluminum with lacquer  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingmat#i" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingmat#"/>
      <skos:prefLabel xml:lang="en">aluminum with lacquer</skos:prefLabel>
      <skos:definition xml:lang="en">Aluminum disc base with a lacquer coating.</skos:definition>
      <skos:notation xml:lang="en">i</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingmat#i"/>
   </skos:Concept>
   <!-- Concept: cellulose nitrate  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingmat#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingmat#"/>
      <skos:prefLabel xml:lang="en">cellulose nitrate</skos:prefLabel>
      <skos:definition xml:lang="en">Cellulose nitrate base.</skos:definition>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:scopeNote xml:lang="en">used for items such as memovox discs.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingmat#b"/>
   </skos:Concept>
   <!-- Concept: glass with lacquer  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingmat#g" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingmat#"/>
      <skos:prefLabel xml:lang="en">glass with lacquer</skos:prefLabel>
      <skos:definition xml:lang="en">Glass disc base with a lacquer coating.</skos:definition>
      <skos:notation xml:lang="en">g</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingmat#g"/>
   </skos:Concept>
   <!-- Concept: lacquer coating  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingmat#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingmat#"/>
      <skos:prefLabel xml:lang="en">lacquer coating</skos:prefLabel>
      <skos:definition xml:lang="en">Lacquer coated, but the base material on which the lacquer is applied is not identified.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingmat#a"/>
   </skos:Concept>
   <!-- Concept: metal  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingmat#l" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingmat#"/>
      <skos:prefLabel xml:lang="en">metal</skos:prefLabel>
      <skos:definition xml:lang="en">Made entirely of metal.</skos:definition>
      <skos:notation xml:lang="en">l</skos:notation>
      <skos:scopeNote xml:lang="en">Includes the earliest cylinders, which were made of tinfoil, and aluminum transcription discs with no coating, as well as wire and metal tape.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingmat#l"/>
   </skos:Concept>
   <!-- Concept: not applicable [obsolete]  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingmat#nx" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingmat#"/>
      <skos:prefLabel xml:lang="en">not applicable [obsolete]</skos:prefLabel>
      <skos:notation xml:lang="en">n</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingmat#nx"/>
   </skos:Concept>
   <!-- Concept: other  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingmat#z" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingmat#"/>
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:definition xml:lang="en">Kind of material other than lacquer coating, cellulose nitrate, acetate tape with ferrous oxide. glass with lacquer, aluminum with lacquer, metal, plastic with metal, plastic, paper with lacquer or ferrous oxide, shellac, wax.</skos:definition>
      <skos:notation xml:lang="en">z</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingmat#z"/>
   </skos:Concept>
   <!-- Concept: paper with lacquer or ferrous oxide  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingmat#r" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingmat#"/>
      <skos:prefLabel xml:lang="en">paper with lacquer or ferrous oxide</skos:prefLabel>
      <skos:definition xml:lang="en">Paper or cardboard disc, tape, or cylinder base with a lacquer or ferrous oxide coating.</skos:definition>
      <skos:notation xml:lang="en">r</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingmat#r"/>
   </skos:Concept>
   <!-- Concept: plastic  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingmat#p" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingmat#"/>
      <skos:prefLabel xml:lang="en">plastic</skos:prefLabel>
      <skos:definition xml:lang="en">Made entirely of plastic.</skos:definition>
      <skos:notation xml:lang="en">p</skos:notation>
      <skos:scopeNote xml:lang="en">Also includes mylar, polyester and vinyl materials. Since the 1950s, commercial or mass-produced sound recordings on 16, 33 1/3, and 45 rpm discs, cassettes, and cartridges are made of plastic.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingmat#p"/>
   </skos:Concept>
   <!-- Concept: plastic with metal  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingmat#m" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingmat#"/>
      <skos:prefLabel xml:lang="en">plastic with metal</skos:prefLabel>
      <skos:definition xml:lang="en">Made of plastic base with a metal coating.</skos:definition>
      <skos:notation xml:lang="en">m</skos:notation>
      <skos:scopeNote xml:lang="en">The metal part of such an item is often a thin layer applied to the plastic base. Includes compact audio discs (CDs).</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingmat#m"/>
   </skos:Concept>
   <!-- Concept: shellac  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingmat#s" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingmat#"/>
      <skos:prefLabel xml:lang="en">shellac</skos:prefLabel>
      <skos:definition xml:lang="en">Made entirely of shellac.</skos:definition>
      <skos:notation xml:lang="en">s</skos:notation>
      <skos:scopeNote xml:lang="en">Most commercial or mass-produced 78 rpm discs are made of shellac.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingmat#s"/>
   </skos:Concept>
   <!-- Concept: unknown  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingmat#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingmat#"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Kind of material used in the manufacture of the item is not known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingmat#u"/>
   </skos:Concept>
   <!-- Concept: wax  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingmat#w" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingmat#"/>
      <skos:prefLabel xml:lang="en">wax</skos:prefLabel>
      <skos:definition xml:lang="en">Made entirely of wax.</skos:definition>
      <skos:notation xml:lang="en">w</skos:notation>
      <skos:scopeNote xml:lang="en">Most instantaneous cylinders were made of wax.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingmat#w"/>
   </skos:Concept>
</rdf:RDF>
