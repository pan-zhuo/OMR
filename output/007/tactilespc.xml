      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">Not braille.</skos:definition>
      <skos:notation xml:lang="en">n</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/tactilespc#n"/>
   </skos:Concept>
   <!-- Concept: other  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/tactilespc#z" xml:lang="en">
