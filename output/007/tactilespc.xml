   <!-- Concept: jumbo or enlarged braille  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/tactilespc#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/tactilespc#"/>
      <skos:prefLabel xml:lang="en">jumbo or enlarged braille</skos:prefLabel>
      <skos:definition xml:lang="en">Jumbo or enlarged braille, which is used for braille students and braille readers with reduced tactile sensitivity.</skos:definition>
