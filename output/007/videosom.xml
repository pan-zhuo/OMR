<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:dct="http://purl.org/dc/terms/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-007: Sound on medium or separate of video -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/videosom#">
      <dct:title>MARC21-007: Sound on medium or separate of video</dct:title>
      <dct:description>To be filled from CSV</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
   </skos:ConceptScheme>
   <!-- Concept: no sound (silent)  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/videosom#%23" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/videosom#"/>
      <skos:prefLabel xml:lang="en">no sound (silent)</skos:prefLabel>
      <skos:definition xml:lang="en">Sound is not present (i.e., the item is intended to be silent).</skos:definition>
      <skos:notation xml:lang="en">#</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/videosom#%23"/>
   </skos:Concept>
   <!-- Concept: sound on medium  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/videosom#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/videosom#"/>
      <skos:prefLabel xml:lang="en">sound on medium</skos:prefLabel>
      <skos:definition xml:lang="en">Sound is on the item, whether or not visual images are included.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/videosom#a"/>
   </skos:Concept>
   <!-- Concept: sound separate from medium  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/videosom#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/videosom#"/>
      <skos:prefLabel xml:lang="en">sound separate from medium</skos:prefLabel>
      <skos:definition xml:lang="en">Sound is on a separate medium, designed to accompany the images.</skos:definition>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/videosom#b"/>
   </skos:Concept>
   <!-- Concept: unknown  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/videosom#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/videosom#"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Presence or absence of sound on the item is not known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/videosom#u"/>
   </skos:Concept>
</rdf:RDF>
