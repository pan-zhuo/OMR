<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:dct="http://purl.org/dc/terms/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-007: Tape width of sound recording -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/soundrecordingwid#">
      <dct:title>MARC21-007: Tape width of sound recording</dct:title>
      <dct:description>To be filled from CSV</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
   </skos:ConceptScheme>
   <!-- Concept: 1 in.  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingwid#p" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingwid#"/>
      <skos:prefLabel xml:lang="en">1 in.</skos:prefLabel>
      <skos:definition xml:lang="en">Recording studios often use 1 inch tape. Some 8-track cartridges have been issued in 1 in. tape.</skos:definition>
      <skos:notation xml:lang="en">p</skos:notation>
      <skos:scopeNote xml:lang="en">Recording studios often use 1 inch tape. Some 8-track cartridges have been issued in 1 in. tape.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingwid#p"/>
   </skos:Concept>
   <!-- Concept: 1/2 in.  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingwid#o" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingwid#"/>
      <skos:prefLabel xml:lang="en">1/2 in.</skos:prefLabel>
      <skos:definition xml:lang="en">1/2 in.</skos:definition>
      <skos:notation xml:lang="en">o</skos:notation>
      <skos:scopeNote xml:lang="en">Recording studios customarily use 1/2 in. tape. Some 8-track cartridges have been issued in 1/2 in. tape.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingwid#o"/>
   </skos:Concept>
   <!-- Concept: 1/4 in.  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingwid#m" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingwid#"/>
      <skos:prefLabel xml:lang="en">1/4 in.</skos:prefLabel>
      <skos:definition xml:lang="en">1/4 in.</skos:definition>
      <skos:notation xml:lang="en">m</skos:notation>
      <skos:scopeNote xml:lang="en">Nonprofessional, consumer, and some professional reels, and most 8-track cartridges use 1/4 in. tape.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingwid#m"/>
   </skos:Concept>
   <!-- Concept: 1/8 in.  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingwid#l" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingwid#"/>
      <skos:prefLabel xml:lang="en">1/8 in.</skos:prefLabel>
      <skos:definition xml:lang="en">1/8 in.</skos:definition>
      <skos:notation xml:lang="en">l</skos:notation>
      <skos:scopeNote xml:lang="en">Most cassettes use 1/8 in. tape.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingwid#l"/>
   </skos:Concept>
   <!-- Concept: not applicable  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingwid#n" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingwid#"/>
      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">Item is not a tape or does not include a tape.</skos:definition>
      <skos:notation xml:lang="en">n</skos:notation>
      <skos:scopeNote xml:lang="en">For example, if the item is a disc.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingwid#n"/>
   </skos:Concept>
   <!-- Concept: other  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingwid#z" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingwid#"/>
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:definition xml:lang="en">Width other than 1/8 in., 1/4 in., 1/2 in., 1 in.</skos:definition>
      <skos:notation xml:lang="en">z</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingwid#z"/>
   </skos:Concept>
   <!-- Concept: unknown  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingwid#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingwid#"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Tape width is not known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingwid#u"/>
   </skos:Concept>
</rdf:RDF>
