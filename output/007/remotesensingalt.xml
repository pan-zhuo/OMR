<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:dct="http://purl.org/dc/terms/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-007: Altitude of remote sensor -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/remotesensingalt#">
      <dct:title>MARC21-007: Altitude of remote sensor</dct:title>
      <dct:description>To be filled from CSV</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
   </skos:ConceptScheme>
   <!-- Concept: airborne  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingalt#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingalt#"/>
      <skos:prefLabel xml:lang="en">airborne</skos:prefLabel>
      <skos:definition xml:lang="en">Remote-sensing image was made from a device above the surface of a body, generally in the atmosphere, an aircraft, balloon, or some other airborne device.</skos:definition>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingalt#b"/>
   </skos:Concept>
   <!-- Concept: not applicable  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingalt#n" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingalt#"/>
      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">Altitude of the sensor is not applicable to the image.</skos:definition>
      <skos:notation xml:lang="en">n</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingalt#n"/>
   </skos:Concept>
   <!-- Concept: other  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingalt#z" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingalt#"/>
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:definition xml:lang="en">Altitude of sensor other than surface, airborne, spaceborne.</skos:definition>
      <skos:notation xml:lang="en">z</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingalt#z"/>
   </skos:Concept>
   <!-- Concept: spaceborne  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingalt#c" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingalt#"/>
      <skos:prefLabel xml:lang="en">spaceborne</skos:prefLabel>
      <skos:definition xml:lang="en">Remote-sensing image was made from a device in space. Spaceborne sensors are those located outside the primary layers of an atmosphere (usually in orbit) and beyond.</skos:definition>
      <skos:notation xml:lang="en">c</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingalt#c"/>
   </skos:Concept>
   <!-- Concept: surface  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingalt#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingalt#"/>
      <skos:prefLabel xml:lang="en">surface</skos:prefLabel>
      <skos:definition xml:lang="en">Remote-sensing image was made from a device located on the surface of a body, usually a planet or moon.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingalt#a"/>
   </skos:Concept>
   <!-- Concept: unknown  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingalt#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingalt#"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Altitude of the sensor from which the image was made is not known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingalt#u"/>
   </skos:Concept>
</rdf:RDF>
