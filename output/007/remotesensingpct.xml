<!-- Scheme: MARC21-007: Platform construction type of remote sensing device -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/remotesensingpct#">
      <dct:title>MARC21-007: Platform construction type of remote sensing device</dct:title>
      <dct:description>To be filled from CSV</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
   </skos:ConceptScheme>
   <!-- Concept: aircraft - high altitude  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingpct#d" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpct#"/>
      <skos:prefLabel xml:lang="en">aircraft - high altitude</skos:prefLabel>
      <skos:definition xml:lang="en">Base for the remote-sensing device was an dynamic lift aircraft designed for high altitude flight (above 49,000 ft. (14,810 m.)).</skos:definition>
      <skos:notation xml:lang="en">d</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingpct#d"/>
   </skos:Concept>
   <!-- Concept: aircraft - low altitude  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingpct#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpct#"/>
      <skos:prefLabel xml:lang="en">aircraft - low altitude</skos:prefLabel>
      <skos:definition xml:lang="en">Base for the remote-sensing device was an dynamic lift aircraft designed for low altitude flight (below 29,500 ft. (8,962 m.)).</skos:definition>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingpct#b"/>
   </skos:Concept>
   <!-- Concept: aircraft - medium altitude  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingpct#c" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpct#"/>
      <skos:prefLabel xml:lang="en">aircraft - medium altitude</skos:prefLabel>
      <skos:definition xml:lang="en">Base for the remote-sensing device was an dynamic lift aircraft designed for medium altitude flight (between 29,500 ft. (8,962 m.) and 49,000 ft. (14,810 m.)).</skos:definition>
      <skos:notation xml:lang="en">c</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingpct#c"/>
   </skos:Concept>
   <!-- Concept: balloon  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingpct#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpct#"/>
      <skos:prefLabel xml:lang="en">balloon</skos:prefLabel>
      <skos:definition xml:lang="en">Base for the remote-sensing device was a balloon or similar lighter-than-air platform.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingpct#a"/>
   </skos:Concept>
   <!-- Concept: land-based remote-sensing device  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingpct#g" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpct#"/>
      <skos:prefLabel xml:lang="en">land-based remote-sensing device</skos:prefLabel>
      <skos:definition xml:lang="en">Base for the remote-sensing device was a land-based.</skos:definition>
      <skos:notation xml:lang="en">g</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingpct#g"/>
   </skos:Concept>
   <!-- Concept: manned spacecraft  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingpct#e" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpct#"/>
      <skos:prefLabel xml:lang="en">manned spacecraft</skos:prefLabel>
      <skos:definition xml:lang="en">Base for the remote-sensing device was a manned spacecraft.</skos:definition>
      <skos:notation xml:lang="en">e</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingpct#e"/>
   </skos:Concept>
   <!-- Concept: not applicable  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingpct#n" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpct#"/>
      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">Not applicable.</skos:definition>
      <skos:notation xml:lang="en">n</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingpct#n"/>
   </skos:Concept>
   <!-- Concept: other  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingpct#z" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpct#"/>
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:definition xml:lang="en">Platform construction type other than balloon, aircraft - low altitude, aircraft - medium altitude, aircraft - high altitude, manned spacecraft, unmanned spacecraft, land-based remote-sensing device, water surface-based remote-sensing device, submersible remote-sensing device.</skos:definition>
      <skos:notation xml:lang="en">z</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingpct#z"/>
   </skos:Concept>
   <!-- Concept: submersible remote-sensing device  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingpct#i" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpct#"/>
      <skos:prefLabel xml:lang="en">submersible remote-sensing device</skos:prefLabel>
      <skos:definition xml:lang="en">Base for the remote-sensing device was designed to be submerged beneath the surface of a body of water</skos:definition>
      <skos:notation xml:lang="en">i</skos:notation>
      <skos:scopeNote xml:lang="en">E.g., a submersible vessel or platform</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingpct#i"/>
   </skos:Concept>
   <!-- Concept: unknown  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingpct#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpct#"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Construction type of the platform from which the remote-sensing image was made is not known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingpct#u"/>
   </skos:Concept>
   <!-- Concept: unmanned spacecraft  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingpct#f" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpct#"/>
      <skos:prefLabel xml:lang="en">unmanned spacecraft</skos:prefLabel>
      <skos:definition xml:lang="en">Base for the remote-sensing device was an unmanned spacecraft.</skos:definition>
      <skos:notation xml:lang="en">f</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingpct#f"/>
   </skos:Concept>
   <!-- Concept: water surface-based remote-sensing device  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/remotesensingpct#h" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpct#"/>
      <skos:prefLabel xml:lang="en">water surface-based remote-sensing device</skos:prefLabel>
      <skos:definition xml:lang="en">Base for the remote-sensing device was designed to remain on the surface of a body of water.</skos:definition>
      <skos:notation xml:lang="en">h</skos:notation>
      <skos:scopeNote xml:lang="en">E.g., a ship or floating platform</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/remotesensingpct#h"/>
   </skos:Concept>
</rdf:RDF>
