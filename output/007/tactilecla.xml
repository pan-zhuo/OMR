      <skos:prefLabel xml:lang="en">mathematics and scientific braille</skos:prefLabel>
      <skos:definition xml:lang="en">Braille representations for symbols employed in mathematics and scientific writing.</skos:definition>
      <skos:notation xml:lang="en">c</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/tactilecla#c"/>
