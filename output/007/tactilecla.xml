      <skos:prefLabel xml:lang="en">literary braille</skos:prefLabel>
      <skos:definition xml:lang="en">Braille representation for the alphabet, numbers, and punctuation marks commonly used in general writing.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/tactilecla#a"/>
