   <!-- Concept: computer braille  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/tactilecla#d" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/tactilecla#"/>
      <skos:prefLabel xml:lang="en">computer braille</skos:prefLabel>
      <skos:definition xml:lang="en">Braille representations for symbols used in computer programs, texts, and other computer related materials.</skos:definition>
      <skos:notation xml:lang="en">d</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/tactilecla#d"/>
   </skos:Concept>
   <!-- Concept: format code braille  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/tactilecla#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/tactilecla#"/>
      <skos:prefLabel xml:lang="en">format code braille</skos:prefLabel>
      <skos:definition xml:lang="en">Braille formatting rules for textbooks and related materials.</skos:definition>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/tactilecla#b"/>
   </skos:Concept>
   <!-- Concept: literary braille  -->
