<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:dct="http://purl.org/dc/terms/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-007: Specific material designation of projected graphic -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/projectedsmd#">
      <dct:title>MARC21-007: Specific material designation of projected graphic</dct:title>
      <dct:description>To be filled from CSV</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
   </skos:ConceptScheme>
   <!-- Concept: filmslip  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/projectedsmd#d" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/projectedsmd#"/>
      <skos:prefLabel xml:lang="en">filmslip</skos:prefLabel>
      <skos:definition xml:lang="en">A short filmstrip not in a roll.</skos:definition>
      <skos:notation xml:lang="en">d</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/projectedsmd#d"/>
   </skos:Concept>
   <!-- Concept: filmstrip cartridge  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/projectedsmd#c" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/projectedsmd#"/>
      <skos:prefLabel xml:lang="en">filmstrip cartridge</skos:prefLabel>
      <skos:definition xml:lang="en">A filmstrip which is encased in a cartridge and which has the ends joined to form a loop. A cartridge denotes a permanently encased single reel of film or tape that has the ends joined together to form a loop that provides playback without rewinding.</skos:definition>
      <skos:notation xml:lang="en">c</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/projectedsmd#c"/>
   </skos:Concept>
   <!-- Concept: filmstrip roll  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/projectedsmd#o" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/projectedsmd#"/>
      <skos:prefLabel xml:lang="en">filmstrip roll</skos:prefLabel>
      <skos:definition xml:lang="en">A roll of film on which still images are recorded, presenting an integrated communication. It is intended for projection frame by frame.</skos:definition>
      <skos:notation xml:lang="en">o</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/projectedsmd#o"/>
   </skos:Concept>
   <!-- Concept: other  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/projectedsmd#z" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/projectedsmd#"/>
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:definition xml:lang="en">Specific material designation other than filmstrip cartridge, filmslip, other filmstrip type, filmstrip roll, slide, transparency.</skos:definition>
      <skos:notation xml:lang="en">z</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/projectedsmd#z"/>
   </skos:Concept>
   <!-- Concept: other filmstrip type  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/projectedsmd#f" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/projectedsmd#"/>
      <skos:prefLabel xml:lang="en">other filmstrip type</skos:prefLabel>
      <skos:definition xml:lang="en">A filmstrip other than a filmstrip cartridge, filmslip or filmstrip roll or is an unspecified type of filmstrip.</skos:definition>
      <skos:notation xml:lang="en">f</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/projectedsmd#f"/>
   </skos:Concept>
   <!-- Concept: slide  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/projectedsmd#s" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/projectedsmd#"/>
      <skos:prefLabel xml:lang="en">slide</skos:prefLabel>
      <skos:definition xml:lang="en">Transparent material on which there is a two-dimensional image, usually held in a mount, and designed for use in a projector or viewer.</skos:definition>
      <skos:notation xml:lang="en">s</skos:notation>
      <skos:scopeNote xml:lang="en">Includes modern stereographs, e.g., Viewmaster reels.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/projectedsmd#s"/>
   </skos:Concept>
   <!-- Concept: transparency  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/projectedsmd#t" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/projectedsmd#"/>
      <skos:prefLabel xml:lang="en">transparency</skos:prefLabel>
      <skos:definition xml:lang="en">Transparent material on which an image is recorded. They are designed for use with a projector or a light box.</skos:definition>
      <skos:notation xml:lang="en">t</skos:notation>
      <skos:scopeNote xml:lang="en">Includes X-rays.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/projectedsmd#t"/>
   </skos:Concept>
   <!-- Concept: unspecified  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/projectedsmd#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/projectedsmd#"/>
      <skos:prefLabel xml:lang="en">unspecified</skos:prefLabel>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/projectedsmd#u"/>
   </skos:Concept>
</rdf:RDF>
