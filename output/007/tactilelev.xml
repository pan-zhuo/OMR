<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:dct="http://purl.org/dc/terms/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-007: Level of contraction of tactile resource -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/tactilelev#">
      <dct:title>MARC21-007: Level of contraction of tactile resource</dct:title>
      <dct:description>To be filled from CSV</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
   </skos:ConceptScheme>
   <!-- Concept: combination  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/tactilelev#m" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/tactilelev#"/>
      <skos:prefLabel xml:lang="en">combination</skos:prefLabel>
      <skos:definition xml:lang="en">A combination of contracted and uncontracted.</skos:definition>
      <skos:notation xml:lang="en">m</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/tactilelev#m"/>
   </skos:Concept>
   <!-- Concept: contracted  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/tactilelev#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/tactilelev#"/>
      <skos:prefLabel xml:lang="en">contracted</skos:prefLabel>
      <skos:definition xml:lang="en">Contracted.</skos:definition>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:scopeNote xml:lang="en">Includes Standard English braille, the code for contracted braille of the Braille Authority of the United Kingdom and English Braille, American ed., the code for contracted braille of the Braille Authority of North America.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/tactilelev#b"/>
   </skos:Concept>
   <!-- Concept: not applicable  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/tactilelev#n" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/tactilelev#"/>
      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">Item is not braille or Moon.</skos:definition>
      <skos:notation xml:lang="en">n</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/tactilelev#n"/>
   </skos:Concept>
   <!-- Concept: other  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/tactilelev#z" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/tactilelev#"/>
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:definition xml:lang="en">Level of contraction other than uncontracted, contracted, combination.</skos:definition>
      <skos:notation xml:lang="en">z</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/tactilelev#z"/>
   </skos:Concept>
   <!-- Concept: uncontracted  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/tactilelev#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/tactilelev#"/>
      <skos:prefLabel xml:lang="en">uncontracted</skos:prefLabel>
      <skos:definition xml:lang="en">Not contracted.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:scopeNote xml:lang="en">Includes grade 1 braille.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/tactilelev#a"/>
   </skos:Concept>
   <!-- Concept: unknown  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/tactilelev#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/tactilelev#"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Level of contraction is not known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/tactilelev#u"/>
   </skos:Concept>
</rdf:RDF>
