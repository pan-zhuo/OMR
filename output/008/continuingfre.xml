      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
   </skos:ConceptScheme>
   <!-- Concept: annual  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/continuingfre#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/continuingfre#"/>
      <skos:prefLabel xml:lang="en">annual</skos:prefLabel>
      <skos:definition xml:lang="en">Once a year.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/continuingfre#a"/>
   </skos:Concept>
   <!-- Concept: biennial  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/continuingfre#g" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/continuingfre#"/>
      <skos:prefLabel xml:lang="en">biennial</skos:prefLabel>
      <skos:definition xml:lang="en">Every two years.</skos:definition>
      <skos:notation xml:lang="en">g</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/continuingfre#g"/>
