<rdf:RDF xmlns:dct="http://purl.org/dc/terms/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-008: Form of item of computer files -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/computerfor#">
      <dct:title>MARC21-008: Form of item of computer files</dct:title>
      <dct:description>Specifies the form of material for the computer file.</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794" />
   </skos:ConceptScheme>
   <!-- Concept: direct electronic  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/computerfor#q" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/computerfor#" />
      <skos:prefLabel xml:lang="en">direct electronic</skos:prefLabel>
      <skos:definition xml:lang="en">Storage on a directly accessible tangible recording medium, e.g. disc, tape, playaway device, flashdrive, portable hard drive, etc.</skos:definition>
      <skos:notation xml:lang="en">q</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/computerfor#q" />
   </skos:Concept>
   <!-- Concept: online  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/computerfor#o" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/computerfor#" />
      <skos:prefLabel xml:lang="en">online</skos:prefLabel>
      <skos:definition xml:lang="en">Accessed by means of hardware and software connections to a communications network.</skos:definition>
      <skos:notation xml:lang="en">o</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/computerfor#o" />
   </skos:Concept>
   <!-- Concept: unknown or not specified  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/computerfor#%23" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/computerfor#" />
      <skos:prefLabel xml:lang="en">unknown or not specified</skos:prefLabel>
      <skos:notation xml:lang="en">#</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/computerfor#%23" />
   </skos:Concept>
</rdf:RDF>