<?xml version='1.0' encoding='UTF-8'?>
<rdf:RDF xmlns:dct="http://purl.org/dc/terms/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-007: Positive/negative aspect of map -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/mappna#">
      <dct:title>MARC21-007: Positive/negative aspect of map</dct:title>
      <dct:description>Positive/negative aspect of the photocopy or film of the cartographic item. This characteristic is also referred to as polarity.</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794" />
   </skos:ConceptScheme>
   <!-- Concept: mixed polarity  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/mappna#m" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/mappna#" />
      <skos:prefLabel xml:lang="en">mixed polarity</skos:prefLabel>
      <skos:definition xml:lang="en">Mixture of positive and negative images.</skos:definition>
      <skos:notation xml:lang="en">m</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/mappna#m" />
   </skos:Concept>
   <!-- Concept: negative  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/mappna#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/mappna#" />
      <skos:prefLabel xml:lang="en">negative</skos:prefLabel>
      <skos:definition xml:lang="en">Lines and characters are light on dark background.</skos:definition>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/mappna#b" />
   </skos:Concept>
   <!-- Concept: not applicable  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/mappna#n" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/mappna#" />
      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">Item is not a photocopy or film.</skos:definition>
      <skos:notation xml:lang="en">n</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/mappna#n" />
   </skos:Concept>
   <!-- Concept: positive  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/mappna#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/mappna#" />
      <skos:prefLabel xml:lang="en">positive</skos:prefLabel>
      <skos:definition xml:lang="en">Lines and characters are dark on light background.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/mappna#a" />
   </skos:Concept>
</rdf:RDF>