<?xml version='1.0' encoding='UTF-8'?>
<rdf:RDF xmlns:dct="http://purl.org/dc/terms/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-007: Specific material designation of globe -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/globesmd#">
      <dct:title>MARC21-007: Specific material designation of globe</dct:title>
      <dct:description>Special class of material (usually the class of physical object) to which an item belongs, e.g., a celestial globe.</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794" />
   </skos:ConceptScheme>
   <!-- Concept: celestial globe  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/globesmd#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/globesmd#" />
      <skos:prefLabel xml:lang="en">celestial globe</skos:prefLabel>
      <skos:definition xml:lang="en">Model relating to the sky of visible heavens (sun, stars, etc.) depicted on the surface of a sphere.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/globesmd#a" />
   </skos:Concept>
   <!-- Concept: earth moon globe  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/globesmd#e" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/globesmd#" />
      <skos:prefLabel xml:lang="en">earth moon globe</skos:prefLabel>
      <skos:definition xml:lang="en">Model of the Earth's moon depicted on the surface of a sphere.</skos:definition>
      <skos:notation xml:lang="en">e</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/globesmd#e" />
   </skos:Concept>
   <!-- Concept: other  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/globesmd#z" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/globesmd#" />
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:definition xml:lang="en">Specific material designation other than celestial globe, planetary or lunar globe, terrestrial globe, Earth moon globe.</skos:definition>
      <skos:notation xml:lang="en">z</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/globesmd#z" />
   </skos:Concept>
   <!-- Concept: planetary or lunar globe  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/globesmd#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/globesmd#" />
      <skos:prefLabel xml:lang="en">planetary or lunar globe</skos:prefLabel>
      <skos:definition xml:lang="en">Lunar globe (other than Earth) or a moon depicted on the surface of a sphere.</skos:definition>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/globesmd#b" />
   </skos:Concept>
   <!-- Concept: terrestrial globe  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/globesmd#c" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/globesmd#" />
      <skos:prefLabel xml:lang="en">terrestrial globe</skos:prefLabel>
      <skos:definition xml:lang="en">Model of the Earth depicted on the surface of a sphere.</skos:definition>
      <skos:notation xml:lang="en">c</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/globesmd#c" />
   </skos:Concept>
   <!-- Concept: unspecified  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/globesmd#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/globesmd#" />
      <skos:prefLabel xml:lang="en">unspecified</skos:prefLabel>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/globesmd#u" />
   </skos:Concept>
</rdf:RDF>