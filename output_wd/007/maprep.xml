<?xml version='1.0' encoding='UTF-8'?>
<rdf:RDF xmlns:dct="http://purl.org/dc/terms/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-007: Type of reproduction of map -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/maprep#">
      <dct:title>MARC21-007: Type of reproduction of map</dct:title>
      <dct:description>Whether the cartographic item is a facsimile or other type of reproduction.</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794" />
   </skos:ConceptScheme>
   <!-- Concept: facsimile  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/maprep#f" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/maprep#" />
      <skos:prefLabel xml:lang="en">facsimile</skos:prefLabel>
      <skos:definition xml:lang="en">Facsimile which resembles the original in all aspects possible, but it is not the original.</skos:definition>
      <skos:notation xml:lang="en">f</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/maprep#f" />
   </skos:Concept>
   <!-- Concept: not applicable  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/maprep#n" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/maprep#" />
      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">Item is not a reproduction.</skos:definition>
      <skos:notation xml:lang="en">n</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/maprep#n" />
   </skos:Concept>
   <!-- Concept: other  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/maprep#z" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/maprep#" />
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:definition xml:lang="en">Type of reproduction other than facsimile.</skos:definition>
      <skos:notation xml:lang="en">z</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/maprep#z" />
   </skos:Concept>
   <!-- Concept: unknown  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/maprep#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/maprep#" />
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Type of reproduction is not known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/maprep#u" />
   </skos:Concept>
</rdf:RDF>