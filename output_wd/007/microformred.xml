<?xml version='1.0' encoding='UTF-8'?>
<rdf:RDF xmlns:dct="http://purl.org/dc/terms/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-007: Reduction ratio range of microform -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/microformred#">
      <dct:title>MARC21-007: Reduction ratio range of microform</dct:title>
      <dct:description>Reduction ratio range for a microform.</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794" />
   </skos:ConceptScheme>
   <!-- Concept: high reduction  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformred#c" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformred#" />
      <skos:prefLabel xml:lang="en">high reduction</skos:prefLabel>
      <skos:definition xml:lang="en">Between 31:1 and 60:1.</skos:definition>
      <skos:notation xml:lang="en">c</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformred#c" />
   </skos:Concept>
   <!-- Concept: low reduction  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformred#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformred#" />
      <skos:prefLabel xml:lang="en">low reduction</skos:prefLabel>
      <skos:definition xml:lang="en">Less than 16:1 ratio.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformred#a" />
   </skos:Concept>
   <!-- Concept: normal reduction  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformred#b" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformred#" />
      <skos:prefLabel xml:lang="en">normal reduction</skos:prefLabel>
      <skos:definition xml:lang="en">Between 16:1 and 30:1.</skos:definition>
      <skos:notation xml:lang="en">b</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformred#b" />
   </skos:Concept>
   <!-- Concept: reduction rate varies  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformred#v" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformred#" />
      <skos:prefLabel xml:lang="en">reduction rate varies</skos:prefLabel>
      <skos:definition xml:lang="en">Not all parts of the microform have the same reduction ratio.</skos:definition>
      <skos:notation xml:lang="en">v</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformred#v" />
   </skos:Concept>
   <!-- Concept: ultra high reduction  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformred#e" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformred#" />
      <skos:prefLabel xml:lang="en">ultra high reduction</skos:prefLabel>
      <skos:definition xml:lang="en">Over 90:1.</skos:definition>
      <skos:notation xml:lang="en">e</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformred#e" />
   </skos:Concept>
   <!-- Concept: unknown  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformred#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformred#" />
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Reduction ratio is not known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformred#u" />
   </skos:Concept>
   <!-- Concept: very high reduction  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/microformred#d" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/microformred#" />
      <skos:prefLabel xml:lang="en">very high reduction</skos:prefLabel>
      <skos:definition xml:lang="en">Between 61:1 and 90:1.</skos:definition>
      <skos:notation xml:lang="en">d</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/microformred#d" />
   </skos:Concept>
</rdf:RDF>