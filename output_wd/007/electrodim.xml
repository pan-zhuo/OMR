<?xml version='1.0' encoding='UTF-8'?>
<rdf:RDF xmlns:dct="http://purl.org/dc/terms/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:skos="http://www.w3.org/2004/02/skos/core#">

<!-- Scheme: MARC21-007: Dimensions of electronic image -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/electrodim#">
      <dct:title>MARC21-007: Dimensions of electronic image</dct:title>
      <dct:description>Dimensions of the medium used to encode the electronic resource.</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794" />
   </skos:ConceptScheme>
   <!-- Concept: 1 1/8 x 2 3/8 in.  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/electrodim#i" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/electrodim#" />
      <skos:prefLabel xml:lang="en">1 1/8 x 2 3/8 in.</skos:prefLabel>
      <skos:definition xml:lang="en">Dimensions of the container used for the medium are 1 1/8 x 2 3/8 inches.</skos:definition>
      <skos:notation xml:lang="en">i</skos:notation>
      <skos:scopeNote xml:lang="en">Usually refers to cartridges.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/electrodim#i" />
   </skos:Concept>
   <!-- Concept: 12 in.  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/electrodim#e" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/electrodim#" />
      <skos:prefLabel xml:lang="en">12 in.</skos:prefLabel>
      <skos:definition xml:lang="en">Diameter of the medium is 12 inches.</skos:definition>
      <skos:notation xml:lang="en">e</skos:notation>
      <skos:scopeNote xml:lang="en">Usually refers to optical or magneto-optical disc.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/electrodim#e" />
   </skos:Concept>
   <!-- Concept: 3 1/2 in.  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/electrodim#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/electrodim#" />
      <skos:prefLabel xml:lang="en">3 1/2 in.</skos:prefLabel>
      <skos:definition xml:lang="en">Diameter of the medium is 3 1/2 inches.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
      <skos:scopeNote xml:lang="en">Usually refers to the diameter of a magnetic disk.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/electrodim#a" />
   </skos:Concept>
   <!-- Concept: 3 7/8 x 2 1/2 in.  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/electrodim#j" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/electrodim#" />
      <skos:prefLabel xml:lang="en">3 7/8 x 2 1/2 in.</skos:prefLabel>
      <skos:definition xml:lang="en">Dimensions of the container used for the medium are 3 7/8 x 2 1/2 inches.</skos:definition>
      <skos:notation xml:lang="en">j</skos:notation>
      <skos:scopeNote xml:lang="en">Usually refers to cartridges.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/electrodim#j" />
   </skos:Concept>
   <!-- Concept: 4 3/4 in. or 12 cm.  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/electrodim#g" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/electrodim#" />
      <skos:prefLabel xml:lang="en">4 3/4 in. or 12 cm.</skos:prefLabel>
      <skos:definition xml:lang="en">Diameter of the medium is 4 3/4 inches (or 12 cm.).</skos:definition>
      <skos:notation xml:lang="en">g</skos:notation>
      <skos:scopeNote xml:lang="en">Usually refers to optical or magneto-optical disc.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/electrodim#g" />
   </skos:Concept>
   <!-- Concept: 5 1/4 in.  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/electrodim#o" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/electrodim#" />
      <skos:prefLabel xml:lang="en">5 1/4 in.</skos:prefLabel>
      <skos:definition xml:lang="en">Diameter of the medium is 5 1/4 inches.</skos:definition>
      <skos:notation xml:lang="en">o</skos:notation>
      <skos:scopeNote xml:lang="en">Usually refers to the dimensions of a magnetic disk.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/electrodim#o" />
   </skos:Concept>
   <!-- Concept: 8 in.  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/electrodim#v" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/electrodim#" />
      <skos:prefLabel xml:lang="en">8 in.</skos:prefLabel>
      <skos:definition xml:lang="en">Diameter of the medium is 8 inches.</skos:definition>
      <skos:notation xml:lang="en">v</skos:notation>
      <skos:scopeNote xml:lang="en">Usually refers to the diameter of a magnetic disk or optical disc.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/electrodim#v" />
   </skos:Concept>
   <!-- Concept: not applicable  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/electrodim#n" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/electrodim#" />
      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">Physical dimensions are not applicable to the electronic resource.</skos:definition>
      <skos:notation xml:lang="en">n</skos:notation>
      <skos:scopeNote xml:lang="en">Appropriate for remote electronic resources and electronic resources whose specific medium varies.</skos:scopeNote>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/electrodim#n" />
   </skos:Concept>
   <!-- Concept: other  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/electrodim#z" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/electrodim#" />
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:definition xml:lang="en">Dimensions other than 3 1/2 in., 12 in., 4 3/4 in. or 12 cm., 1 1/8 x 2 3/8 in., 3 7/8 x 2 1/2 in., 5 1/4 in., 8 in.</skos:definition>
      <skos:notation xml:lang="en">z</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/electrodim#z" />
   </skos:Concept>
   <!-- Concept: unknown  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/electrodim#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/electrodim#" />
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Dimensions of the medium used for the electronic resource are not known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/electrodim#u" />
   </skos:Concept>
</rdf:RDF>